<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="ISO-8859-1">
<title>Add Bank Account</title>
</head>
<body>
<div>
<pre>
  <form action="" method="POST">
  <h1>Bank Account Details</h1>
   <label>BANK NAME :</label>
  <input  type="text" name="bankName" placeholder="Enter Your Bank Name"><br>
   <label>ACCOUNT NUMBER :</label>
  <input type="text" name="accountNumber" placeholder="Enter Your Account Number"><br>
   <label>IFSC CODE :</label>
  <input type="text" name="ifscCode"  placeholder="Enter Yout IFSC code"><br>
   <label>ACCOUNT BALANCE :</label>
  <input type="text" name="accountBalance" placeholder="Enter Your Bank Balance"><br>
   <label>ACCOUNT STATUS :</label>
  <input type="text" name="accountStatus" placeholder="Bank Account Status"><br>
   <label>ACCOUNT TYPE :</label>  
  <input type="text" name="accountType" placeholder="Enter Account Type"><br>
   <label>LOCATION :</label>
  <input type="text" name="bankLocation" placeholder="Bank Location"><br>
  
  <input type="button" value="submit">
  </form>
  </pre>
  
</div>
</body>
</html>